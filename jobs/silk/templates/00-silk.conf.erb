{
  "name": "cni-wrapper",
  "type": "cni-wrapper-plugin",
  "cniVersion": "0.3.0",
  "datastore": "<%= p('cni_datastore_path') %>",
  "iptables_lock_file": "<%= p('cni_iptables_lock_file') %>",
  "overlay_network": "<%= link("silk-controller").p('network') %>",
  "delegate": {
    "name": "silk",
    "type": "flannel",
    "subnetFile": "<%= p('flannel_subnet_file') %>",
    "dataDir": "<%= p('flannel_data_dir') %>",
    "delegate": {
      "bridge": "cni-flannel0",
      "isDefaultGateway": true,
      "ipMasq": false
     }
  }
}
